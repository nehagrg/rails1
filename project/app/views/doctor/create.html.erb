<%= form_for @doctor,url: { action: "create_doctor"}, htnl: {class: "doctor"} do|d| %>
  <%= d.label :name %>
  <%= d.text_field :name %>
  <%= d.label :age %>
  <%= d.text_field :age %>
  <%= d.label :gender %>
  <%= d.radio_button :gender,"male" %>
  <%= d.label :male %>
  <%= d.radio_button :gender,"female" %>
  <%= d.label :female %>
  <%= d.submit :create_doctor %>
<% end %>    